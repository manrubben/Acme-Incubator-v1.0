	</bean>
	
	<!-- SanabriaBulletin -->
	
	<bean id="SanabriaBulletin1" class="acme.entities.sanabriaBulletins.SanabriaBulletin">
		<property name="author" value="Manolo"></property>
		<property name="song" value="La Macarena"></property>
	</bean>
	<bean id="SanabriaBulletin2" class="acme.entities.sanabriaBulletins.SanabriaBulletin">
		<property name="author" value="Manu"></property>
