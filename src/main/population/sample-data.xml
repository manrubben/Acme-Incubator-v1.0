<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="conversion-service.xml" />
	
	
	<!-- DiazBulletin -->
	
	
	<bean id="Bulletin01" class="acme.entities.diazBulletins.DiazBulletin">
		<property name="surname" value="Diaz"></property>
		<property name="bulletin" value="Añade una foto"></property>
	</bean>

	<bean id="Bulletin02" class="acme.entities.diazBulletins.DiazBulletin">
		<property name="surname" value="Sanabria"></property>
		<property name="bulletin" value="El banner ha quedado bien"></property>
		
	</bean>

	<bean id="Bulletin03" class="acme.entities.diazBulletins.DiazBulletin">
		<property name="surname" value="Rubio"></property>
		<property name="bulletin" value="Todo funciona correctamente"></property>
	</bean>


	<!-- RubioBulletin -->
	
	<bean id="RubioBulletin1" class="acme.entities.rubioBulletins.RubioBulletin">
		<property name="author" value="Carles gil"></property>
		<property name="link" value="http://www.rubiobulletin.com"></property>
	</bean>
	
	<bean id="RubioBulletin2" class="acme.entities.rubioBulletins.RubioBulletin">
		<property name="author" value="Antonio Perez"></property>
		<property name="link" value="http://www.bulletins.com"></property>
	</bean>
	
